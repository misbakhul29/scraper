    "@prisma/client": "^7.2.0",
    "amqplib": "^0.10.3",
    "axios": "^1.13.2",
    "dotenv": "^16.6.1",
    "express": "^4.21.1",
    "pg": "^8.16.3",
    "puppeteer": "^23.6.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.0",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "prisma": "^7.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
